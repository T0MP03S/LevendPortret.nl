{
  "name": "levendportret-monorepo",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "scripts": {
    "build": "dotenv -e .env.local -- turbo run build",
    "dev": "dotenv -e .env.local -- turbo run dev",
    "dev:web": "dotenv -e .env.local -- pnpm -C apps/web dev",
    "dev:club": "dotenv -e .env.local -- pnpm -C apps/club dev",
    "dev:fund": "dotenv -e .env.local -- pnpm -C apps/fund dev",
    "dev:admin": "dotenv -e .env.local -- pnpm -C apps/admin dev",
    "lint": "turbo run lint",
    "prisma:generate": "dotenv -e .env.local -- pnpm --filter @levendportret/db run prisma:generate",
    "prisma:migrate:dev": "dotenv -e .env.local -- pnpm --filter @levendportret/db run prisma:migrate:dev",
    "prisma:studio": "dotenv -e .env.local -- pnpm --filter @levendportret/db run prisma:studio"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^7.0.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "pnpm": "^9.11.0",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.10",
    "zod": "^4.1.12"
  }
}
